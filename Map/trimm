fastp --in1 00_raw/SampleID_R1.fastq.gz --in2 00_raw/SampleID_R2.fastq.gz 
--out1 ./02_trimmed/SampleID_1_trimmed.fastq.gz 
--out2 ./02_trimmed/SampleID_2_trimmed.fastq.gz 
